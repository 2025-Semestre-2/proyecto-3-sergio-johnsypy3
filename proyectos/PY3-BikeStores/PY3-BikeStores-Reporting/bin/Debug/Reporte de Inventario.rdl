  <DataSets>
    <DataSet Name="Sucursal">
      <Query>
        <DataSourceName>BikeStores</DataSourceName>
        <CommandText>SELECT Sucursal
FROM (
    SELECT 'Todas las Sucursales' AS Sucursal, 0 AS Orden
    UNION ALL
    SELECT s.store_name as Sucursal, 1 AS Orden
    FROM sales.stores s
) AS X
ORDER BY Orden, Sucursal;</CommandText>
      </Query>
      <Fields>
        <Field Name="Sucursal">
          <DataField>Sucursal</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Categoria">
      <Query>
        <DataSourceName>BikeStores</DataSourceName>
        <CommandText>SELECT Categoria
FROM (
    SELECT 'Todas las Categorias' AS Categoria, 0 AS Orden
    UNION ALL
    SELECT c.category_name AS Categoria, 1 AS Orden
    FROM production.categories c
) AS X
ORDER BY Orden, Categoria;</CommandText>
      </Query>
      <Fields>
        <Field Name="Categoria">
          <DataField>Categoria</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Marca">
      <Query>
        <DataSourceName>BikeStores</DataSourceName>
        <CommandText>=IIf(
    Parameters!Sucursal.Value = "Todas las Sucursales" And Parameters!Categoria.Value = "Todas las Categorias",

    "SELECT Brand FROM (
         SELECT 'Todas las Marcas' AS Brand, 0 AS Orden
         UNION ALL
         SELECT DISTINCT b.brand_name AS Brand, 1 AS Orden
         FROM production.brands b
         INNER JOIN production.products p ON p.brand_id = b.brand_id
     ) AS X
     ORDER BY Orden, Brand",

IIf(
    Parameters!Sucursal.Value = "Todas las Sucursales" And Parameters!Categoria.Value &lt;&gt; "Todas las Categorias",

    "SELECT Brand FROM (
         SELECT 'Todas las Marcas' AS Brand, 0 AS Orden
         UNION ALL
         SELECT DISTINCT b.brand_name AS Brand, 1 AS Orden
         FROM production.brands b
         INNER JOIN production.products p ON p.brand_id = b.brand_id
         INNER JOIN production.categories c ON c.category_id = p.category_id
         WHERE c.category_name = '" &amp; Parameters!Categoria.Value &amp; "'
     ) AS X
     ORDER BY Orden, Brand",

IIf(
    Parameters!Sucursal.Value &lt;&gt; "Todas las Sucursales" And Parameters!Categoria.Value = "Todas las Categorias",

    "SELECT Brand FROM (
         SELECT 'Todas las Marcas' AS Brand, 0 AS Orden
         UNION ALL
         SELECT DISTINCT b.brand_name AS Brand, 1 AS Orden
         FROM production.brands b
         INNER JOIN production.products p ON p.brand_id = b.brand_id
         INNER JOIN sales.order_items oi ON oi.product_id = p.product_id
         INNER JOIN sales.orders o ON o.order_id = oi.order_id
         INNER JOIN sales.stores s ON s.store_id = o.store_id
         WHERE s.store_name = '" &amp; Parameters!Sucursal.Value &amp; "'
     ) AS X
     ORDER BY Orden, Brand",

    "SELECT Brand FROM (
         SELECT 'Todas las Marcas' AS Brand, 0 AS Orden
         UNION ALL
         SELECT DISTINCT b.brand_name AS Brand, 1 AS Orden
         FROM production.brands b
         INNER JOIN production.products p ON p.brand_id = b.brand_id
         INNER JOIN production.categories c ON c.category_id = p.category_id
         INNER JOIN sales.order_items oi ON oi.product_id = p.product_id
         INNER JOIN sales.orders o ON o.order_id = oi.order_id
         INNER JOIN sales.stores s ON s.store_id = o.store_id
         WHERE c.category_name = '" &amp; Parameters!Categoria.Value &amp; "' 
           AND s.store_name = '" &amp; Parameters!Sucursal.Value &amp; "'
     ) AS X
     ORDER BY Orden, Brand"
)))</CommandText>
      </Query>
      <Fields>
        <Field Name="Brand">
          <DataField>Brand</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="TotalInventarioProductoMarca">
      <Query>
        <DataSourceName>BikeStores</DataSourceName>
        <CommandText>=IIf(
    Parameters!Sucursal.Value = "Todas las Sucursales" And Parameters!Categoria.Value = "Todas las Categorias" And Parameters!Marca.Value = "Todas las Marcas",

    "SELECT p.product_name AS Producto, b.brand_name AS Marca, SUM(st.quantity) AS Cantidad FROM production.stocks st INNER JOIN production.products p ON st.product_id = p.product_id INNER JOIN sales.stores s ON st.store_id = s.store_id INNER JOIN production.categories c ON p.category_id = c.category_id INNER JOIN production.brands b ON p.brand_id = b.brand_id GROUP BY p.product_name, b.brand_name",

IIf(
    Parameters!Sucursal.Value = "Todas las Sucursales" And Parameters!Categoria.Value = "Todas las Categorias" And Parameters!Marca.Value &lt;&gt; "Todas las Marcas",

    "SELECT p.product_name AS Producto, b.brand_name AS Marca, SUM(st.quantity) AS Cantidad FROM production.stocks st INNER JOIN production.products p ON st.product_id = p.product_id INNER JOIN sales.stores s ON st.store_id = s.store_id INNER JOIN production.categories c ON p.category_id = c.category_id INNER JOIN production.brands b ON p.brand_id = b.brand_id WHERE b.brand_name = '" &amp; Parameters!Marca.Value &amp; "' GROUP BY p.product_name, b.brand_name",

IIf(
    Parameters!Sucursal.Value = "Todas las Sucursales" And Parameters!Categoria.Value &lt;&gt; "Todas las Categorias" And Parameters!Marca.Value = "Todas las Marcas",

    "SELECT p.product_name AS Producto, b.brand_name AS Marca, SUM(st.quantity) AS Cantidad FROM production.stocks st INNER JOIN production.products p ON st.product_id = p.product_id INNER JOIN sales.stores s ON st.store_id = s.store_id INNER JOIN production.categories c ON p.category_id = c.category_id INNER JOIN production.brands b ON p.brand_id = b.brand_id WHERE c.category_name = '" &amp; Parameters!Categoria.Value &amp; "' GROUP BY p.product_name, b.brand_name",

IIf(
    Parameters!Sucursal.Value = "Todas las Sucursales" And Parameters!Categoria.Value &lt;&gt; "Todas las Categorias" And Parameters!Marca.Value &lt;&gt; "Todas las Marcas",

    "SELECT p.product_name AS Producto, b.brand_name AS Marca, SUM(st.quantity) AS Cantidad FROM production.stocks st INNER JOIN production.products p ON st.product_id = p.product_id INNER JOIN sales.stores s ON st.store_id = s.store_id INNER JOIN production.categories c ON p.category_id = c.category_id INNER JOIN production.brands b ON p.brand_id = b.brand_id WHERE c.category_name = '" &amp; Parameters!Categoria.Value &amp; "' AND b.brand_name = '" &amp; Parameters!Marca.Value &amp; "' GROUP BY p.product_name, b.brand_name",

IIf(
    Parameters!Sucursal.Value &lt;&gt; "Todas las Sucursales" And Parameters!Categoria.Value = "Todas las Categorias" And Parameters!Marca.Value = "Todas las Marcas",

    "SELECT p.product_name AS Producto, b.brand_name AS Marca, SUM(st.quantity) AS Cantidad FROM production.stocks st INNER JOIN production.products p ON st.product_id = p.product_id INNER JOIN sales.stores s ON st.store_id = s.store_id INNER JOIN production.categories c ON p.category_id = c.category_id INNER JOIN production.brands b ON p.brand_id = b.brand_id WHERE s.store_name = '" &amp; Parameters!Sucursal.Value &amp; "' GROUP BY p.product_name, b.brand_name",

IIf(
    Parameters!Sucursal.Value &lt;&gt; "Todas las Sucursales" And Parameters!Categoria.Value = "Todas las Categorias" And Parameters!Marca.Value &lt;&gt; "Todas las Marcas",

    "SELECT p.product_name AS Producto, b.brand_name AS Marca, SUM(st.quantity) AS Cantidad FROM production.stocks st INNER JOIN production.products p ON st.product_id = p.product_id INNER JOIN sales.stores s ON st.store_id = s.store_id INNER JOIN production.categories c ON p.category_id = c.category_id INNER JOIN production.brands b ON p.brand_id = b.brand_id WHERE s.store_name = '" &amp; Parameters!Sucursal.Value &amp; "' AND b.brand_name = '" &amp; Parameters!Marca.Value &amp; "' GROUP BY p.product_name, b.brand_name",

IIf(
    Parameters!Sucursal.Value &lt;&gt; "Todas las Sucursales" And Parameters!Categoria.Value &lt;&gt; "Todas las Categorias" And Parameters!Marca.Value = "Todas las Marcas",

    "SELECT p.product_name AS Producto, b.brand_name AS Marca, SUM(st.quantity) AS Cantidad FROM production.stocks st INNER JOIN production.products p ON st.product_id = p.product_id INNER JOIN sales.stores s ON st.store_id = s.store_id INNER JOIN production.categories c ON p.category_id = c.category_id INNER JOIN production.brands b ON p.brand_id = b.brand_id WHERE s.store_name = '" &amp; Parameters!Sucursal.Value &amp; "' AND c.category_name = '" &amp; Parameters!Categoria.Value &amp; "' GROUP BY p.product_name, b.brand_name",

    "SELECT p.product_name AS Producto, b.brand_name AS Marca, SUM(st.quantity) AS Cantidad FROM production.stocks st INNER JOIN production.products p ON st.product_id = p.product_id INNER JOIN sales.stores s ON st.store_id = s.store_id INNER JOIN production.categories c ON p.category_id = c.category_id INNER JOIN production.brands b ON p.brand_id = b.brand_id WHERE s.store_name = '" &amp; Parameters!Sucursal.Value &amp; "' AND c.category_name = '" &amp; Parameters!Categoria.Value &amp; "' AND b.brand_name = '" &amp; Parameters!Marca.Value &amp; "' GROUP BY p.product_name, b.brand_name"
)))))))</CommandText>
      </Query>
